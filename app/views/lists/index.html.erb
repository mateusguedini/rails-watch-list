<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://images.unsplash.com/photo-1524985069026-dd778a71c7b4?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2071&q=80);">
  <div class="container">
    <h1 class="d-flex justify-content-center">Movies</h1>
    <p class="d-flex justify-content-center"><%= link_to "New List", new_list_path, class: "btn btn-gradient" %></p>
  </div>
</div>

<div class="cards">
  <% @lists.each do |list| %>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(data:image/jpeg;base64,)">
    <a><%= link_to list.name, list %></a>
    </div>
  <% end %>
</div>
